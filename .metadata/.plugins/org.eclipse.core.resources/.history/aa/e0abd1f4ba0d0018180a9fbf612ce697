import static java.lang.System.*;

public class StringChecker
{
	String word;

   public void setString(String s)
   {
   	word=s;
   }

	public boolean findLetter(char c)
	{
		if(word.indexOf(c)!=-1)
		{
		   return true;
		}
		else
		{
		   return false;
		}
	}

	public boolean findSubString(String s)
	{



		return false;
	}

 	public String toString()
 	{
 		return "\n\n";
	}
}