import static java.lang.System.*;

public class TriangleThree
{
   private int size;
   private String letter, letter2;

	public void setTriangle( String let, int sz )
	{
		letter = let;
		size = sz;
	}

	public String getLetter()
	{
		return "#";
	}

	public String toString()
	{
		String output = "";
		while (size >= 1)
		{
			output = output + " ";
			output = output + letter;
			output = output + "\n";
		}
		return output;
	}
}